{
  "name": "vscode-depcheck",
  "publisher": "juliensanmartin",
  "displayName": "DepCheck",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/package.json"
  ],
  "contributes": {
    "colors": [
      {
        "id": "myextension.unusedDependencies",
        "description": "Background decoration color for unused dependencies",
        "defaults": {
          "dark": "#FF000055",
          "light": "#FF000055",
          "highContrast": "#FF000055"
        }
      }
    ]
  },
  "main": "src/extension.js",
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.39.0",
    "eslint": "^5.13.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "depcheck": "^0.8.3"
  }
}
