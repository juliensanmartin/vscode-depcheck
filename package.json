{
  "name": "vscode-depcheck",
  "publisher": "juliensanmartin",
  "displayName": "DepCheck",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/package.json"
  ],
  "contributes": {
    "colors": [
      {
        "id": "myextension.unusedDependencies",
        "description": "Background decoration color for unused dependencies",
        "defaults": {
          "dark": "#FF000055",
          "light": "#FF000055",
          "highContrast": "#FF000055"
        }
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^8.9.1",
    "tslint": "^5.16.0",
    "typescript": "^3.5.1",
    "@types/vscode": "^1.32.0"
  },
  "dependencies": {
    "depcheck": "^0.8.3"
  }
}
